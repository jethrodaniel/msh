# .gitattributes

# Tell git how to manage the generated files
#
# - `diff=word`         - treat generated files as binary
# - `diff=word`         - show diffs on these like normal text
# - `linguist-vendored` - tell Github to not count these towards repo langs
#
readme.md         binary diff=word linguist-vendored
man/*.adoc        binary diff=word linguist-vendored
man/*.[0-9]       binary diff=word linguist-vendored
lib/msh/parser.rb binary diff=word linguist-vendored
lib/msh/lexer.rb  binary diff=word linguist-vendored
